// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Repository {
  key   Bytes[] @id
  value Bytes

  @@ignore
}

model User {
  id    String @id @default(uuid())
  email String @unique @db.VarChar(255)
}
